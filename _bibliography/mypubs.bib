	in-cv = {1},
	in-website = {1},
	keywords = {Image classification; Neural networks; Medical image and signal analysis; proj-digipath; showcase},
	month = {January},
	pages = {7},
	self = {1},
	status = {-},
	title = {Triplet mining, BUT in a dynamic manner: Improving batch-incremental triplet sampling for training triplet networks utilizing Bayesian updating theorem BUT in a dynamic manner: Improving batch-incremental triplet sampling for training triplet networks utilizing Bayesian updating theorem},
