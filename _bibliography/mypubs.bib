@inproceedings{sikaroudi2021aa,
	abstract = {Triplet networks are very powerful networks for learning an  embedding subspace which discriminates the classes of data. There exist different triplet mining approaches for selecting or generating the best suitable training triplets. Some of these mining methods are based on the extreme distances of instances and some make use of sampling. However, sampling from stochastic distributions of data rather than sampling merely from the existing embedding instances can provide more information for discrimination of classes. In this work, we sample triplets from distributions of data rather than the existing instances. We consider a multivariate normal distribution for the embedding of each class. Using Bayesian updating and conjugate priors, we update the distributions of classes dynamically by receiving the new mini-batches of training data. Two approaches, named Bayesian Updating Triplet (BUT) and Bayesian Updating NCA (BUNCA), are proposed. Experimental results on MNIST and histopathology colorectal cancer (CRC) datasets validate the effectiveness of the proposed sampling approaches. 
},
	address = {Milan, Italy},
	author = {Sikaroudi, Milad and Ghojogh, Benyamin and Karray, Fakhri and Crowley, Mark and Tizhoosh, H.R.},
	booktitle = {The 25th International Conference on Pattern Recognition (ICPR 2020)},
